            </class>
        </classes>
    </test>

    <test name="09 - LogIn Test">
        <classes>
            <class name="LoginPageTest">
                <methods>
                    <include name="logInProblemUserTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="10 - LogIn Test">
        <classes>
            <class name="LoginPageTest">
                <methods>
                    <include name="logInPerformanceGlitchUserTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>